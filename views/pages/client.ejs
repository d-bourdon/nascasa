<% include ../includes/header %>
    <main>
    <% if (locals.query && locals.query.error) { %>
<div class="alert alert-<%= query.error %> alert-dismissible fade in" role="alert"><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">Ã—</span></button> <%= query.msgError %> </div>
        <% } %>
        <div class="row" style="margin-right: 0; margin-left: 0;">
            <% clients.forEach(function(client) { %>
                <div class="col-xs-6 col-md-3">
                    <a href="image/<%= client._id %>" class="thumbnail">
                        <img src="data:image/png;base64,<%= client.base_f %>">
                    </a>
                </div>
            <% }); %>
            <% if (empty == 1) { %>
                <div class="center-block"> <h2><span class="label label-default center-block">Pas d'images disponible</span></h2></div>
            <% }; %>
        </div>
    </main>
<% include ../includes/footer %>